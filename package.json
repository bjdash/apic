{
  "name": "apic-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=5000",
    "start-hmr": "ng serve --port=5000 --hmr",
    "start1": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extnLocal": "gulp cleanExtnLocal && ng build --configuration=extnLocal --base-href=index.html --output-path=dist/extnLocal --stats-json && gulp buildExtnLocal",
    "extn": "gulp cleanExtn && ng build --configuration=extn --base-href=index.html --output-path=dist/extn && gulp buildExtn",
    "electron": "gulp cleanElectron && ng build --configuration=electron --output-path=dist/win && gulp buildElectron && electron-builder build --win --x64",
    "electronLocal": "gulp cleanElectron && ng build --configuration=electronLocal --output-path=dist/win && gulp buildElectron && electron-builder build --win --x64"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.3",
    "@angular/cdk": "^11.2.2",
    "@angular/common": "~11.2.3",
    "@angular/compiler": "~11.2.3",
    "@angular/core": "~11.2.3",
    "@angular/forms": "~11.2.3",
    "@angular/material": "^11.2.2",
    "@angular/platform-browser": "~11.2.3",
    "@angular/platform-browser-dynamic": "~11.2.3",
    "@angular/router": "~11.2.3",
    "@ngxs/store": "^3.6.2",
    "@stomp/rx-stomp": "^1.1.0",
    "ajv": "^7.2.1",
    "crypto-js": "^4.1.1",
    "expression-eval": "^4.0.0",
    "idb": "^5.0.4",
    "js-yaml": "^4.0.0",
    "ng-recaptcha": "^8.0.0",
    "ng2-ace-editor": "^0.3.9",
    "openapi-types": "^9.0.3",
    "rxjs": "~6.5.4",
    "socket.io-client": "^2.4.0",
    "sockjs-client": "^1.5.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.2",
    "@angular/cli": "~11.2.2",
    "@angular/compiler-cli": "~11.2.3",
    "@ngxs/devtools-plugin": "^3.7.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.20.15",
    "codelyzer": "^6.0.0",
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "gulp": "^4.0.2",
    "gulp-git": "^2.10.1",
    "gulp-run": "^1.7.1",
    "gulp-text-simple": "^0.5.5",
    "gulp-zip": "^5.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  },
  "build": {
    "appId": "bj.apic.app",
    "publish": [
      {
        "provider": "generic",
        "url": "https://apic.app/download/"
      }
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "nsis": {
      "perMachine": true,
      "allowElevation": true,
      "deleteAppDataOnUninstall": true,
      "oneClick": false
    },
    "win": {
      "target": "nsis",
      "verifyUpdateCodeSignature": false
    },
    "linux": {
      "target": "AppImage",
      "category": "Development"
    },
    "directories": {
      "buildResources": "platform_files/electron/resources",
      "app": "dist/win/",
      "output": "dist/native"
    }
  }
}
